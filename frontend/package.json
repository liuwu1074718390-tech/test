{
	"name": "gz-vue-base",
	"version": "1.0.0",
	"description": "Vue3 前端基础框架",
	"author": "lizs",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "npm run sethook && vite",
		"build:prod": "vite build",
		"build:stage": "vite build --mode staging",
		"build:test": "vite build --mode test",
		"build:demo": "vite build --mode demo",
		"build:analyze": "vite build --mode analyze",
		"preview": "vite preview",
		"prepare": "husky install",
		"lint": "npm run lint:eslint && npm run lint:stylelint",
		"lint:eslint": "eslint --fix src",
		"lint:stylelint": "stylelint --cache --fix \"src/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
		"lint:lint-staged": "lint-staged",
		"commit": "git-cz",
		"useless-preinstall": "npx only-allow pnpm",
		"clean": "npx rimraf node_modules",
		"clean:cache": "npx rimraf node_modules/.cache",
		"sethook": "node createHuskyHooks.cjs"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"dependencies": {
		"@element-plus/icons-vue": "2.3.1",
		"@tinymce/tinymce-vue": "^6.0.1",
		"@vueup/vue-quill": "^1.2.0",
		"@vueuse/core": "10.6.1",
		"axios": "0.27.2",
		"echarts": "5.4.3",
		"element-plus": "2.7.2",
		"file-saver": "2.0.5",
		"fuse.js": "6.6.2",
		"js-base64": "^3.7.7",
		"js-cookie": "3.0.5",
		"nprogress": "0.2.0",
		"pinia": "2.1.7",
		"sortablejs": "^1.15.2",
		"tinymce": "^7.3.0",
		"update": "^0.7.4",
		"vue": "3.4.26",
		"vue-cropper": "1.1.1",
		"vue-router": "4.2.5",
		"vue3-puzzle-vcode": "^1.1.7",
		"vue3-tree-org": "4.1.0-beta.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@eslint/js": "^9.2.0",
		"@vitejs/plugin-vue": "4.5.0",
		"@vue/compiler-sfc": "3.3.9",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"commitizen": "^4.3.0",
		"commitlint": "^19.3.0",
		"crypto-js": "^4.2.0",
		"cz-conventional-changelog": "^3.3.0",
		"dompurify": "^3.1.5",
		"eslint": "^9.2.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-vue": "^9.25.0",
		"globals": "^15.1.0",
		"husky": "^9.0.11",
		"jsencrypt": "3.3.2",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.38",
		"postcss-html": "^1.6.0",
		"prettier": "^3.2.5",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "1.69.5",
		"sass-loader": "^14.2.1",
		"stylelint": "^16.5.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^5.0.1",
		"stylelint-config-recommended-scss": "^14.0.0",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-config-standard-scss": "^13.1.0",
		"terser": "^5.36.0",
		"unplugin-auto-import": "0.17.1",
		"unplugin-vue-setup-extend-plus": "1.0.0",
		"vite": "5.0.4",
		"vite-plugin-compression": "0.5.1",
		"vite-plugin-svg-icons": "2.0.1"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
